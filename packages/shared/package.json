{
  "name": "@iohook/shared",
  "version": "1.0.2",
  "type": "module",
  "description": "Shared utilities for iohook packages",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.mjs"
    },
    "./event-emitter": {
      "require": "./event-emitter.cjs",
      "import": "./event-emitter.mjs"
    },
    "./event-types": {
      "require": "./event-types.cjs",
      "import": "./event-types.mjs"
    },
    "./logger": {
      "require": "./logger.cjs",
      "import": "./logger.mjs"
    }
  },
  "files": [
    "*.mjs",
    "*.cjs",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "iohook",
    "shared",
    "utilities",
    "event-emitter",
    "logger"
  ],
  "author": "hwanyong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hwanyong/iohook-macos.git",
    "directory": "packages/shared"
  }
}
